<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jmb.view.LeaderboardView">
   <children>
      <TableView fx:id="table" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="128.0" text="NOME" />
          <TableColumn fx:id="victories" minWidth="0.0" prefWidth="109.0" sortType="DESCENDING" text="Vittorie" />
            <TableColumn fx:id="defeats" minWidth="0.0" prefWidth="109.0" sortType="DESCENDING" text="Sconfitte" />
            <TableColumn fx:id="rate" minWidth="0.0" prefWidth="109.0" sortType="DESCENDING" text="Tasso vittorie" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
